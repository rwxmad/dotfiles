# --------------------------------------------------------------------------------------------------
# ZSH
# --------------------------------------------------------------------------------------------------

export ZDOTDIR="$HOME/.config/zsh"

# --------------------------------------------------------------------------------------------------
# Brew
# --------------------------------------------------------------------------------------------------

export PATH=/opt/homebrew/bin:$PATH

# --------------------------------------------------------------------------------------------------
# Starship
# --------------------------------------------------------------------------------------------------

export STARSHIP_CONFIG=~/.config/starship/config.toml

# --------------------------------------------------------------------------------------------------
# Lang
# --------------------------------------------------------------------------------------------------

export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8

# --------------------------------------------------------------------------------------------------
# Cargo
# --------------------------------------------------------------------------------------------------

. "$HOME/.cargo/env"
